<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>StoneAge Technologies</Authors>
    <Company>StoneAge Technologies</Company>
    <Description>Designed to help with testing of rabbitmq stuff. Makes use of RabbitMQ.Fakes.DotNetCore.
The class is a wrapper of RabbitMqContext simular to Ef context object. This implemenation wraps the production RabbitMqContext from RabbitMQ.Context</Description>
    <PackageReleaseNotes>Inital release.</PackageReleaseNotes>
    <AssemblyName>RabbitMq.TestContext</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FluentAssertions" Version="5.7.0" />
    <PackageReference Include="RabbitMQ.Context" Version="1.0.1" />
    <PackageReference Include="RabbitMQ.Fakes.DotNetCore" Version="0.9.1" />
    <PackageReference Include="StoneAge.System.Utils.DotNetCore" Version="1.5.0" />
  </ItemGroup>

</Project>
